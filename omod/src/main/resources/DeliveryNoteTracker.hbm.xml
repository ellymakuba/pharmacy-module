<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.openmrs.module.pharmacy.model.DeliveryNoteTracker" table="pharmacy_delivery_note_tracker">
        <cache usage="read-write"/>

        <id name="id" type="int" column="delivery_note_tracker_id" unsaved-value="0">
            <generator class="org.openmrs.api.db.hibernate.NativeIfNotAssignedIdentityGenerator"/>
        </id>
        <discriminator column="delivery_note_tracker_id" insert="false"/>
        <many-to-one name="pharmacyLocation"  property-ref="uuid" column="location_uuid" not-null="true"/>
        <many-to-one name="creator" class="org.openmrs.User" column="creator" not-null="true"/>
        <property name="deliveryNoteNumber" type="java.lang.Integer" column="delivery_note_number"/>
        <many-to-one name="pharmacySupplier" property-ref="uuid" column="supplier" not-null="true"/>
        <property name="dateCreated" type="java.util.Date" column="datecreated" not-null="true"/>
        <property name="voided" type="boolean" not-null="true"/>
        <many-to-one name="voidedBy" class="org.openmrs.User" column="voided_by"/>
        <property name="dateVoided" type="java.util.Date" column="date_voided"/>
        <property name="voidReason" type="java.lang.String" column="void_reason" length="255"/>
        <property name="uuid" type="string" length="38" not-null="true" unique="true"/>
    </class>
</hibernate-mapping>