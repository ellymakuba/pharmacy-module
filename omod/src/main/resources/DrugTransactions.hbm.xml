<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.openmrs.module.pharmacy.model.DrugTransactions"
           table="pharmacy_drug_transactions">
        <cache usage="read-write"/>

        <id name="id" type="int" column="pharmacy_drug_transactions_id" unsaved-value="0">
         <generator class="org.openmrs.api.db.hibernate.NativeIfNotAssignedIdentityGenerator"/>
        </id>
        <discriminator column="pharmacy_drug_transactions_id" insert="false"/>
        <many-to-one name="drugs"  property-ref="uuid" column="pharmacy_drug_uuid"/>
        <many-to-one name="category" property-ref="uuid" column="pharmacy_category_uuid"/>
        <property name="quantityIn" type="java.lang.Integer" column="quantity_in" length="255"/>
        <property name="quantityOut" type="java.lang.Integer" column="quantity_out" length="255"/>
        <property name="comment" type="java.lang.String" column="comment" length="255"/>
        <property name="s11" type="java.lang.String" column="s11_no" length="255"/>
        <property name="batchNo" type="java.lang.String" column="batchNo" length="255"/>
         <many-to-one name="creator" class="org.openmrs.User" column="creator" not-null="true"/>
        <many-to-one name="location"  property-ref="uuid" column="location_uuid"/>
        <many-to-one name="otherTransactingSite"  property-ref="uuid" column="other_transacting_site" not-null="true"/>
        <property name="expireDate" type="java.util.Date" column="expiredate"  not-null="false"/>
        <property name="dateCreated" type="java.util.Date" column="datecreated" not-null="true"/>
        <property name="voided" type="boolean" not-null="true"/>
        <many-to-one name="voidedBy" class="org.openmrs.User" column="voided_by"/>
        <property name="dateVoided" type="java.util.Date" column="date_voided"/>
        <property name="voidReason" type="java.lang.String" column="void_reason" length="255"/>
        <property name="uuid" type="string" length="38" not-null="true" unique="true"/>
        <property name="approved" type="boolean" not-null="true"/>
    </class>
</hibernate-mapping>